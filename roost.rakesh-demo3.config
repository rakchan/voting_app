build:
  type: text
  content: |-
    #!/bin/sh
    cd $ROOST_WORK_DIR/git/roost-io/ballot
    export IMAGE_TAG=$ROOST_NAMESPACE
    export PATH=$PATH:/usr/bin:/usr/local/bin
    make build-ballot
